<div class="modal" id="createTypeModal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Create transportation type</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="createTypeX">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form method="POST" action="/transportation/type/create" id="createTypeForm">
          <fieldset>
            <div class="form-group">
              <label for="name">Name</label>
              <input type="text" class="form-control" id="name" name="name" placeholder="Enter name">
            </div>
            </fieldset>
          </fieldset>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" id="createTypeButton">Create</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal" id="createTypeClose">Close</button>
      </div>
    </div>
  </div>
</div>