<div class="modal" id="editRoutestationModal">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Edit rotuestation</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="editRoutestationX">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form method="GET" action="/transportation/routestation/edit" id="editRoutestationForm">
            <fieldset>
              <div class="form-group">
                <label for="routeSelectEdit">Route</label>
                <select class="form-control" id="routeSelectEdit" name="routeSelectEdit">
                </select>
              </div>
              <div class="form-group">
                <label for="stationSelectEdit">Station</label>
                <select class="form-control" id="stationSelectEdit" name="stationSelectEdit">
                </select>
              </div>
              <div class="form-group">
                <label for="vehicleSelectEdit">Vehicle</label>
                <select class="form-control" id="vehicleSelectEdit" name="vehicleSelectEdit">
                </select>
              </div>
              <div class="form-group">
                <label for="timeEdit">Time</label>
                <input type="text" class="form-control" id="timeEdit" name="timeEdit"  placeholder="Time">
              </div>
              <div class="form-group">
                <label for="typeEdit">Type</label>
                <input type="text" class="form-control" id="typeEdit" name="typeEdit"  placeholder="Type">
              </div>
            </fieldset>
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" id="editRoutestationButton">Edit</button>
          <button type="button" class="btn btn-secondary" data-dismiss="modal" id="editRoutestationClose">Close</button>
        </div>
      </div>
    </div>
  </div>