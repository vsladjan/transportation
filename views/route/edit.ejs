<div class="modal" id="editRouteModal">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Edit route</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="editRouteX">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form method="GET" action="/transportation/route/edit" id="editRouteForm">
            <fieldset>
              <div class="form-group">
                <label for="idEdit">Id</label>
                <input type="text" readonly class="form-control" id="idEdit" name="id" placeholder="Id">
              </div>
              <div class="form-group">
                <label for="name">Name</label>
                <input type="text" class="form-control" id="nameEdit" name="name" placeholder="Enter name">
              </div>
              <div class="form-group">
                <label for="population">Description</label>
                <input type="text" class="form-control" id="descriptionEdit" name="description"  placeholder="Description">
              </div>
            </fieldset>
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" id="editRouteButton">Edit</button>
          <button type="button" class="btn btn-secondary" data-dismiss="modal" id="editRouteClose">Close</button>
        </div>
      </div>
    </div>
  </div>