<div class="modal" id="editCityModal">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Edit city</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="editCityX">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form method="POST" action="/transportation/city/edit" id="editCityForm">
            <fieldset>
              <div class="form-group">
                <label for="idEdit">Id</label>
                <input type="text" readonly class="form-control" id="idEdit" name="id" placeholder="Id">
              </div>
              <div class="form-group">
                <label for="name">Name</label>
                <input type="text" class="form-control" id="nameEdit" name="name" placeholder="Enter name">
              </div>
              <div class="form-group">
                <label for="population">Population</label>
                <input type="text" class="form-control" id="populationEdit" name="population"  placeholder="Population">
              </div>
              <div class="form-group">
                <label for="size">Size</label>
                <input type="text" class="form-control" id="sizeEdit" name="size"  placeholder="Size">
              </div>
              <div class="form-group">
                <label for="countrySelect">Country</label>
                <select class="form-control" id="countrySelectEdit" name="countrySelect">
                </select>
              </div>
            </fieldset>
          </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" id="editCityButton">Edit</button>
          <button type="button" class="btn btn-secondary" data-dismiss="modal" id="editCityClose">Close</button>
        </div>
      </div>
    </div>
  </div>